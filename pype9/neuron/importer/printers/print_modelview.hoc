/* This file prints out the structure and mechanisms of the model */
objref nil
proc textout() { local i
    printf("%s\n", $o1.s)
    if ($o1.children != nil) {
        for i=0, $o1.children.count-1 {
            textout($o1.children.object(i))
        }
    }
}
load_file("mview.hoc")
objref m
m = new ModelView(0)
for i=0, m.display.top.count-1 {
   textout(m.display.top.object(i))
}
m.destroy()