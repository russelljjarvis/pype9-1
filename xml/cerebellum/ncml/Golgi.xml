                <ionicCurrent id="Golgi_KA" segmentGroup="soma_group">
                    <defun name="alp_a">
                        <arg name="v" />
                        <body> alp_a = tcorr*Aalpha_a*sigm(v-V0alpha_a,Kalpha_a)
                        </body>
                    </defun>
                    <defun name="bet_a">
                        <arg name="v" />
                        <body> tcorr*Abeta_a/(exp((v-V0beta_a)/Kbeta_a))
