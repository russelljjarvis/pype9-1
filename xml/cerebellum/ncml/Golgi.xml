                        <arg name="v" />
                        <body> tcorr*Abeta_b*sigm(v-V0beta_b,Kbeta_b)
                        </body>
                    </defun>
                    <defun name="linoid">
                        <arg name="x" />
                        <arg name="y" />
                        <body> if (fabs(x/y) &lt; 1e-6) { linoid = y*(1 - x/y/2) }else{ linoid =
                            x/(exp(x/y) - 1) }
                        </body>
                    </defun>
                    <defun name="sigm">
                        <arg name="x" />
                        <arg name="y" />
                        <body> 1/(exp(x/y) + 1)
                        </body>
                    </defun>
