                    <defun name="tau">
                        <arg name="potential" />
                        <body> exp(((t1 * potential) - t2)*t3)
                        </body>
                    </defun>
                    <defun name="o_inf">
                        <arg name="potential" />
                        <arg name="Ehalf" />
                        <arg name="c" />
                        <body> 1 / (1 + exp((potential - Ehalf) * c))
